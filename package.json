{
  "name": "ymltotest",
	"displayName": "ymltotest",
	"description": "Convert yml to a it.todo test skeletons",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ymltotest.testify"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ymltotest.testify",
				"title": "yml to test: Convert Selection to Test skeleton"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.5",
		"@types/mocha": "^9.0.0",
		"@types/prettier": "^2.4.2",
		"@types/vscode": "^1.63.0",
		"@types/webpack-env": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-web": "^0.0.15",
		"assert": "^2.0.0",
		"eslint": "^8.1.0",
		"mocha": "^9.1.3",
		"process": "^0.11.10",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.4",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"js-yaml": "^4.1.0",
		"prettier": "2.5.1"
	}
}
